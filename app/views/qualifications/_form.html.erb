<%= form_for(@qualification) do |f| %>

<%= error_explanation_for @qualification %>
 
    <%= f.hidden_field :level_description_id %>

  <div class="field">
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </div>
  <div class="field">
    <%= f.label :statement %><br />
    <%= f.text_area :statement, :rows => 2 %>
  </div>
  <div class="field">
    <%= f.label :partial %><br />
    <%= f.check_box :partial %>
  </div>
 
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
